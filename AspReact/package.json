{
  "name": "AspReact",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "node node_modules/webpack/bin/webpack.js -d --watch --progress --colors",
    "watch:server": "nodemon \"server/server.js\" --watch \"./server\"",
    "test": "jest",
    "lint": "tslint ClientApp/components ClientApp/models ClientApp/store ClientApp/utils || true"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@types/bootstrap": "^4.1.2",
    "@types/enzyme": "^3.1.14",
    "@types/history": "4.6.0",
    "@types/node": "^10.9.4",
    "@types/react": "~15.0.35",
    "@types/react-dom": "~15.5.1",
    "@types/react-hot-loader": "3.0.3",
    "@types/react-redux": "4.4.45",
    "@types/react-router": "4.0.12",
    "@types/react-router-dom": "4.0.5",
    "@types/react-router-redux": "5.0.3",
    "@types/webpack": "2.2.15",
    "@types/webpack-env": "1.13.0",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.1",
    "aspnet-webpack-react": "^3.0.0",
    "awesome-typescript-loader": "3.2.1",
    "bootstrap": "3.3.7",
    "css-loader": "0.28.4",
    "domain-task": "^3.0.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-15": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "event-source-polyfill": "0.0.9",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "history": "4.6.3",
    "jest": "^23.6.0",
    "jquery": "3.2.1",
    "node-noop": "1.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-test-renderer": "^15.6.2",
    "redux": "3.7.1",
    "redux-thunk": "2.2.0",
    "style-loader": "0.18.2",
    "ts-jest": "^23.10.4",
    "typescript": "^2.9.2",
    "url-loader": "0.5.9",
    "webpack": "2.5.1",
    "webpack-hot-middleware": "2.18.2",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "@types/enzyme-adapter-react-15": "^1.0.3",
    "@types/jest": "^23.3.5",
    "@types/jquery": "^3.3.10",
    "@types/toastr": "^2.1.35",
    "toastr": "^2.1.4",
    "wolfy87-eventemitter": "^5.2.5"
  }
}
